@model Pasantes.Website.Models.BodyRight

@if (Model == null)
{
    <h3>No data source found!</h3>
}
else
{
    <div class="container">
        <div class="row">
            <div class="col-sm-12 col-md-12 col-lg-12 col-xl-12">
                <div>
                    <h1>@Model.Title</h1>
                    <p>@Model.Text</p>
                </div>
            </div>
        </div>
    </div>
}